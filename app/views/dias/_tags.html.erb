<% tags = get_string_tags(atividade_form.object.cartao) %>
Editar tags: <%= text_field_tag :tags,"", {class: "tag_autocomplete", onfocus: "tag_autocomplete_apply();",
    value: tags, name: "cartao[#{atividade_form.object.try(:cartao).try(:trello_id)}][tags]"} %> 
  <%= link_to('Resetar Tags','javascript:void(0)',
    {:id => (atividade_form.object.try(:cartao).try(:trello_id)).to_s + "_card", :class => "_card",
      :onclick => 'updateTags(this);'}) %>